<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".ActivityResultados">

    <TextView
        android:id="@+id/textViewSueldoDescontado"
        android:layout_width="195dp"
        android:layout_height="35dp"
        android:layout_marginStart="26dp"
        android:layout_marginTop="64dp"
        app:layout_constraintStart_toEndOf="@+id/imageView5"
        app:layout_constraintTop_toBottomOf="@+id/textViewCargo" />

    <TextView
        android:id="@+id/textViewCargo"
        android:layout_width="90dp"
        android:layout_height="30dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="49dp"
        app:layout_constraintStart_toEndOf="@+id/imageView4"
        app:layout_constraintTop_toBottomOf="@+id/textViewApellidos" />

    <TextView
        android:id="@+id/textViewApellidos"
        android:layout_width="90dp"
        android:layout_height="30dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="53dp"
        app:layout_constraintStart_toEndOf="@+id/imageView3"
        app:layout_constraintTop_toBottomOf="@+id/textViewNombres" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="68dp"
        android:layout_height="60dp"
        android:layout_marginTop="48dp"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/nombre"
        tools:layout_editor_absoluteX="25dp" />

    <TextView
        android:id="@+id/textViewNombres"
        android:layout_width="90dp"
        android:layout_height="30dp"
        android:layout_marginStart="22dp"
        android:layout_marginTop="69dp"
        app:layout_constraintStart_toEndOf="@+id/imageView"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="66dp"
        android:layout_height="56dp"
        android:layout_marginTop="33dp"
        app:layout_constraintTop_toBottomOf="@+id/imageView"
        app:srcCompat="@drawable/apellido"
        tools:layout_editor_absoluteX="25dp" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="66dp"
        android:layout_height="56dp"
        android:layout_marginTop="23dp"
        app:layout_constraintTop_toBottomOf="@+id/imageView3"
        app:srcCompat="@drawable/cargo"
        tools:layout_editor_absoluteX="25dp" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="64dp"
        android:layout_height="70dp"
        android:layout_marginTop="30dp"
        app:layout_constraintTop_toBottomOf="@+id/imageView4"
        app:srcCompat="@drawable/sueldo"
        tools:layout_editor_absoluteX="25dp" />

    <TextView
        android:id="@+id/valorDiario"
        android:layout_width="159dp"
        android:layout_height="62dp"
        android:layout_marginStart="137dp"
        android:layout_marginTop="84dp"
        android:layout_marginEnd="160dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewSueldoDescontado" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="65dp"
        android:layout_height="61dp"
        android:layout_marginTop="51dp"
        app:layout_constraintTop_toBottomOf="@+id/imageView5"
        app:srcCompat="@drawable/valordia"
        tools:layout_editor_absoluteX="25dp" />

</androidx.constraintlayout.widget.ConstraintLayout>